package academic.DTO;

public class SubjectDTO {
	private int id;
	private String name;
	
	
	private SubjectDTO(int id, String name) {
		super();
		this.id = id;
		this.name = name;
	}
	
	private int getId() {
		return id;
	}
	private void setId(int id) {
		this.id = id;
	}
	private String getName() {
		return name;
	}
	private void setName(String name) {
		this.name = name;
	}
	
}
